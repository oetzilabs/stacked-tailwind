{
  "name": "web-fullstack-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "predev": "yarn dev:docker",
    "dev": "yarn dev:next",
    "dev:next": "next dev",
    "dev:docker": "zx ./docker.mjs",
    "dev:migrate": "prisma migrate dev --name",
    "dev:generate": "prisma generate",
    "dev:seed": "yarn prisma db seed",
    "build:deploy": "prisma migrate deploy",
    "prebuild": "concurrently -c green.bold,blue.bold 'yarn:dev:generate' 'yarn:build:deploy'",
    "build": "next build",
    "postinstall": "concurrently -c green.bold,blue.bold 'yarn:dev:docker' 'yarn:dev:generate' && yarn build:deploy",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "license": "MIT",
  "keywords": [
    "web",
    "fullstack",
    "starter"
  ],
  "author": "Özgür Isbert",
  "repository": {
    "url": "https://github.com/oezguerisbert/web-fullstack-starter.git"
  },
  "dependencies": {
    "@fontsource/inter": "4.5.7",
    "@next-auth/prisma-adapter": "1.0.3",
    "@prisma/client": "3.12.0",
    "@trpc/client": "9.23.0",
    "@trpc/next": "9.23.0",
    "@trpc/react": "9.23.0",
    "@trpc/server": "9.23.0",
    "framer-motion": "6.3.1",
    "next": "12.1.6",
    "next-auth": "4.3.3",
    "next-seo": "5.4.0",
    "next-themes": "^0.1.1",
    "nprogress": "0.2.0",
    "phosphor-react": "1.4.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-query": "3.35.0",
    "superjson": "1.8.1",
    "zod": "3.14.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.24",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.1",
    "@types/react-dom": "18.0.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.1.0",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.4",
    "postcss": "^8.4.13",
    "prisma": "3.13.0",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "4.6.3",
    "zx": "6.1.0"
  }
}
